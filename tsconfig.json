{
  "compilerOptions": {
    "target": "ES6",
    "module": "Node16",
    "noEmit": true,
    "rootDir": "./src",
    "strict": true,
    "resolveJsonModule": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "moduleResolution": "Node16"
  },
  "include": [
    "src/**/*.d.ts",
    "src/**/*.ts",
    "src/**/*.json",
    "src/data/licenses/gmp/LICENSE",
    "src/data/licenses/icu/LICENSE",
    "src/data/licenses/zlib/LICENSE"
  ]
}
