{
  "name": "setup-agda",
  "version": "0.1.0",
  "private": true,
  "description": "Set up a specific version of Agda.",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "update-package-info": "ts-node ./src/package-info/update-package-info.ts",
    "all": "npm run update-package-info && npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wenkokke/setup-agda.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Wen Kokke",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^5.1.1",
    "@actions/glob": "^0.3.0",
    "@actions/io": "^1.1.2",
    "@actions/tool-cache": "^2.0.1",
    "@types/semver": "^7.3.12",
    "appdirsjs": "^1.2.7",
    "semver": "^7.3.8",
    "setup-haskell": "file:./vendor/haskell/actions/setup"
  },
  "devDependencies": {
    "@types/node": "^18.8.3",
    "@typescript-eslint/parser": "^5.39.0",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.24.0",
    "eslint-plugin-github": "^4.3.7",
    "eslint-plugin-jest": "^27.1.0",
    "jest": "^27.2.5",
    "js-yaml": "^4.1.0",
    "prettier": "2.7.1",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
